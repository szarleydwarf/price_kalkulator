<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="eu.rjch.kalkulatorcen">

	
	<application
			android:allowBackup="true"
			android:icon="@mipmap/ic_l"
			android:label="@string/app_name"
			android:roundIcon="@mipmap/ic_launcher_round"
			android:supportsRtl="true"
			android:theme="@style/AppTheme">
		
		<meta-data
				android:name="com.google.android.gms.ads.APPLICATION_ID"
				android:value="@string/ad_mob_id"/>
		<activity
				android:name=".MainA"
				android:screenOrientation="portrait"
				android:theme="@android:style/Theme.NoTitleBar.Fullscreen">
			<intent-filter>
				<action android:name="android.intent.action.MAIN"/>
				
				<category android:name="android.intent.category.LAUNCHER"/>
			</intent-filter>
		</activity>
		<activity android:name=".activities.AppSetup"
		          android:screenOrientation="portrait"
		          android:theme="@android:style/Theme.NoTitleBar.Fullscreen">
		</activity>
		<activity android:name=".activities.TheApp"
		          android:screenOrientation="portrait"
		          android:theme="@android:style/Theme.NoTitleBar.Fullscreen">
		</activity>
		<activity android:name=".activities.EulaAct"
		          android:screenOrientation="portrait"
		          android:theme="@android:style/Theme.NoTitleBar.Fullscreen">
		</activity>
	</application>

</manifest>